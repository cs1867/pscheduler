{
    "tasks": {
	"pscheduler-tool-nuttcp-tcp": {
	    "_meta": {
		"display-name": "Throughput - Nuttcp TCP"
	    },
	    "group": "mesh",
	    "schedule": "PT4H",
	    "test": "pscheduler-tool-nuttcp-tcp",
	    "tools": [ "nuttcp" ]
	},
	"pscheduler-tool-nuttcp-udp": {
	    "_meta": {
		"display-name": "Throughput - Nuttcp UDP"
	    },
	    "group": "mesh",
	    "schedule": "PT4H",
	    "test": "pscheduler-tool-nuttcp-udp",
	    "tools": [ "nuttcp" ]
	}

    },
    "tests": {
	"pscheduler-tool-nuttcp-tcp": {
	    "type": "throughput",
	    "spec": {
		"source": "{% address[0] %}",
		"dest": "{% address[1] %}",
		"duration": "PT20S",
		"bandwidth": 50000000
	    }
	},
	"pscheduler-tool-nuttcp-udp": {
	    "type": "throughput",
	    "spec": {
		"udp": true,
		"source": "{% address[0] %}",
		"dest": "{% address[1] %}",
		"duration": "PT20S",
		"bandwidth": 50000000
	    }
	}	
    }
}
